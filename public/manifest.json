{
  "manifest_version": 3,
  "name": "Infoharvest Extension",
  "version": "0.1",
  "description": "Set InfoHarvest as your default new tab page for a seamless browsing experience",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "favicon/favicon-16x16.png",
    "32": "favicon/favicon-32x32.png",
    "48": "/favicon/favicon-32x32.png",
    "192": "favicon/android-chrome-192x192.png",
    "512": "favicon/android-chrome-512x512.png"
  }
}
